// aux
predicate CLIENT_ID         object(key cid)
rewrite CLIENT_ID(u) ‚ü∂      USER_NAME(u, client_user_name) ‚äó USER_ZONE_NAME(u, client_zone)
rewrite classical CLIENT_ID(u) ‚ü∂      USER_NAME(u, client_user_name) ‚àß USER_ZONE_NAME(u, client_zone)

predicate META              object(key oid, key mid, property a, property v, property u)
rewrite META(x, m, a, v, u) ‚ü∂ OBJT_METAMAP_OBJ(x, m) ‚äó META_ATTR_UNIT(m, u) ‚äó META_ATTR_NAME(m, a) ‚äó META_ATTR_VALUE(m, v)
rewrite classical META(x, m, a, v, u) ‚ü∂ OBJT_METAMAP_OBJ(x, m) ‚àß META_ATTR_UNIT(m, u) ‚àß META_ATTR_NAME(m, a) ‚àß META_ATTR_VALUE(m, v)

predicate META_ATTR_ACCESS_OBJ object(key a, key oid, key uid, key acc)
rewrite META_ATTR_ACCESS_OBJ(oid, uid, acc) ‚ü∂  OBJT_ACCESS_OBJ(oid, uid, acc)
rewrite classical META_ATTR_ACCESS_OBJ(oid, uid, acc) ‚ü∂  OBJT_ACCESS_OBJ(oid, uid, acc)

// new predicates
predicate DATA_NAME_2       object(key oid, property n)
rewrite DATA_NAME_2(x, y) ‚ü∂   DATA_NAME(x, y) ‚äó CLIENT_ID(u) ‚äó OBJT_ACCESS_OBJ(x, u, a) ‚äó eq(a, 1200)
rewrite classical DATA_NAME_2(x, y) ‚ü∂   DATA_NAME(x, y) ‚àß CLIENT_ID(u) ‚àß OBJT_ACCESS_OBJ(x, u, a) ‚àß eq(a, 1200)
rewrite insert DATA_NAME_2(x, y)  ‚ü∂  insert DATA_NAME(x, y)
rewrite delete DATA_NAME_2(x, y)  ‚ü∂  delete DATA_NAME(x, y)

predicate META_1            object(key oid, key a, key v, key u)
rewrite META_1(x, a, v, u) ‚ü∂  META(x, m, a, v, u)
rewrite classical META_1(x, a, v, u) ‚ü∂  META(x, m, a, v, u)
rewrite insert META_1(x, a, v, u) ‚ü∂  nextid(m) ‚äó insert
                            OBJT_METAMAP_OBJ(x, m) META_OBJ(m) META_ATTR_UNIT(m, u) META_ATTR_NAME(m, a) META_ATTR_VALUE(m, v)
rewrite delete META_1(x, a, v, u) ‚ü∂  OBJT_METAMAP_OBJ(x, m) ‚äó META_ATTR_UNIT(m, u) ‚äó META_ATTR_NAME(m, a) ‚äó META_ATTR_VALUE(m, v) ‚äó delete
                            OBJT_METAMAP_OBJ(x, m)

predicate META_2            object(key oid, key a, key v, key u)
rewrite classical META_2(x, a, v, u) ‚ü∂  META(x, m, a, v, u) ‚àß CLIENT_ID(user) ‚àß CYPHER_META_ACCESS_OBJ(m, x, user, acc) ‚àß eq(acc, 1200)
rewrite META_2(x, a, v, u) ‚ü∂  META(x, m, a, v, u) ‚äó CLIENT_ID(user) ‚äó CYPHER_META_ACCESS_OBJ(m, x, user, acc) ‚äó eq(acc, 1200)
rewrite insert META_2(x, a, v, u) ‚ü∂  nextid(m) ‚äó CLIENT_ID(user) ‚äó META_ATTR_ACCESS_OBJ(x, user, acc) ‚äó eq(acc,1200) ‚äó
                            ([ ¬¨ CYPHER_USER_OBJ(user) ] ‚äó insert CYPHER_USER_OBJ(user) ‚äó ùüé ‚äï
                             [ ¬¨ CYPHER_DATA_OBJ(x) ] ‚äó insert CYPHER_DATA_OBJ(x) ‚äó ùüé ‚äï
                             insert OBJT_METAMAP_OBJ(x, m) META_OBJ(m) META_ATTR_UNIT(m, u) META_ATTR_NAME(m, a) META_ATTR_VALUE(m, v)
                             CYPHER_META_OBJ(m) CYPHER_META_ACCESS_OBJ(m, x, user, 1200))
rewrite delete META_2(x, a, v, u) ‚ü∂  CLIENT_ID(user) ‚äó CYPHER_META_ACCESS_OBJ(m, x, user, acc) ‚äó eq(acc, 1200) ‚äó
                            OBJT_METAMAP_OBJ(x, m) ‚äó META_ATTR_UNIT(m, u) ‚äó META_ATTR_NAME(m, a) ‚äó META_ATTR_VALUE(m, v) ‚äó
                            delete OBJT_METAMAP_OBJ(x, m) CYPHER_META_OBJ(m) CYPHER_META_ACCESS_OBJ(m, x, user, acc)
